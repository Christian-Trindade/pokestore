"95"],"/home/christian/projetos/pokestore/src/pages/index.tsx",["96"],"import React, { useEffect, useContext } from \"react\";\nimport { BrowserRouter as Router, Switch, Route } from \"react-router-dom\";\n\n// contexts\nimport { StoreContext } from \"../services/contexts\";\nimport { ThemeProvider } from \"styled-components\";\n\nimport Themes from \"../themes\";\n\n// pages\nimport Home from \"./home\";\n\nconst StoreType = getStoreType();\n\nfunction getStoreType(): string {\n  let paramsSplited = window.location.host.split(\".\");\n  let type = paramsSplited[0];\n\n  return type;\n}\n\nconst App: React.FC = () => {\n  const context = useContext(StoreContext);\n\n  useEffect(() => {\n    document.title = context.title;\n  }, []);\n\n  return (\n    <Router>\n      <Switch>\n        <Route path=\"/\">\n          <Home />\n        </Route>\n      </Switch>\n    </Router>\n  );\n};\n\nconst AppContexted: React.FC = () => {\n  return (\n    <StoreContext.Provider value={Themes[StoreType].data}>\n      <ThemeProvider theme={Themes[StoreType].pallet}>\n        <App />\n      </ThemeProvider>\n    </StoreContext.Provider>\n  );\n};\n\nexport default AppContexted;\n",["97","98"],"/home/christian/projetos/pokestore/src/reportWebVitals.ts",[],["99","100"],"/home/christian/projetos/pokestore/src/themes/index.tsx",[],"/home/christian/projetos/pokestore/src/pages/home/index.tsx",[],"/home/christian/projetos/pokestore/src/pages/home/components/ui/index.tsx",["101"],"import styled from \"styled-components\";\nimport TextField from \"@material-ui/core/TextField\";\nimport Popover from \"@material-ui/core/Popover\";\n\nexport const Container = styled.div`\n  width: 100%;\n  height: 100%;\n`;\n\nexport const Header = styled.div`\n  background-color: ${({ theme }) => theme.primary};\n  color: ${({ theme }) => theme.titleColor};\n  min-width: 100%;\n  display: flex;\n  flex-wrap: wrap;\n\n  #start-container {\n    width: 205px;\n    display: flex;\n    flex-direction: row;\n    box-